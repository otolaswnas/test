    scrape_interval: 5s
    static_configs:
      - targets: ['localhost:9091']
    relabel_configs:
      - source_labels: [__address__]
        target_label: instance
        replacement: app1
    basic_auth:
      username: 'user1'
      password: 'password1'

  - job_name: 'app2'
    scrape_interval: 5s
    static_configs:
      - targets: ['localhost:9092']
    relabel_configs:
      - source_labels: [__address__]
        target_label: instance
        replacement: app2
    basic_auth:
      username: 'user2'
      password: 'password2'

